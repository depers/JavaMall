<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bravedawn.system.mapper.SmsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.bravedawn.system.entity.Sms">
        <id column="id" property="id" />
        <result column="batchid" property="batchid" />
        <result column="phone" property="phone" />
        <result column="message" property="message" />
        <result column="state" property="state" />
        <result column="send_time" property="sendTime" />
        <result column="priority" property="priority" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, batchid, phone, message, state, send_time, priority
    </sql>

</mapper>
